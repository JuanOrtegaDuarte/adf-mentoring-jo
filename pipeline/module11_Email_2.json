									"BillingDuration": {
										"value": {
											"value": "@activity('CopyCSVToSql').output.billingReference.billableDuration[0].duration",
											"type": "Expression"
										},
										"type": "Decimal"
									},
									"CopyDuration": {
										"value": {
											"value": "@activity('CopyCSVToSql').output.copyDuration",
											"type": "Expression"
