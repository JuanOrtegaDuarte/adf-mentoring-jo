									"PipelineRunId": {
										"value": {
											"value": "@pipeline().RunId",
											"type": "Expression"
										},
										"type": "String"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "MoviesAzSqlDatabaseLS",
								"type": "LinkedServiceReference"
							}
						}
